<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="PixelOS"
           fetch="https://github.com/PixelOS-AOSP"
           revision="refs/heads/fourteen" />

  <remote  name="PixelOS-Gitlab"
           fetch="https://gitlab.com/PixelOS-AOSP"
           revision="refs/heads/fourteen" />

  <remote  name="PixelOS-Devices-Gitlab"
           fetch="https://gitlab.com/PixelOS-Devices"
           revision="refs/heads/fourteen" />

  <remote  name="gl"
           fetch="https://github.com/uwu-gl"
           revision="refs/heads/fourteen" />

  <!-- Rootdir -->
  <project path="bionic" name="bionic" groups="pdk" remote="PixelOS" />

  <!-- Bootable -->
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="PixelOS" />

  <!-- Build -->
  <project path="build/make" name="build_make" groups="pdk" remote="gl">
    <linkfile dest="build/CleanSpec.mk" src="CleanSpec.mk" />
    <linkfile dest="build/buildspec.mk.default" src="buildspec.mk.default" />
    <linkfile dest="build/core" src="core" />
    <linkfile dest="build/envsetup.sh" src="envsetup.sh" />
    <linkfile dest="build/target" src="target" />
    <linkfile dest="build/tools" src="tools" />
  </project>
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="gl">
    <linkfile dest="Android.bp" src="root.bp" />
    <linkfile dest="bootstrap.bash" src="bootstrap.bash" />
  </project>

  <!-- External -->
  <project path="external/jemalloc_new" name="external_jemalloc_new" groups="pdk" remote="PixelOS" />

  <!-- Frameworks -->
  <project path="frameworks/av" name="frameworks_av" groups="pdk" remote="PixelOS" />
  <project path="frameworks/base" name="pixelos-frameworks_base" groups="pdk-cw-fs,pdk-fs" remote="gl" />
  <project path="frameworks/native" name="frameworks_native" groups="pdk" remote="PixelOS" />

  <!-- Hardware -->
  <project path="hardware/google/pixel-sepolicy" name="hardware_google_pixel-sepolicy" groups="generic_fs,pixel" remote="PixelOS" />

  <!-- Packages -->
  <project path="packages/apps/Settings" name="packages_apps_Settings" groups="pdk-fs" remote="PixelOS" />
  <project path="packages/modules/Connectivity" name="packages_modules_Connectivity" groups="pdk" remote="PixelOS" />
  <project path="packages/services/Telecomm" name="packages_services_Telecomm" remote="PixelOS" />

  <!-- System -->
  <project path="system/core" name="system_core" groups="pdk" remote="PixelOS" />
  <project path="system/sepolicy" name="system_sepolicy" groups="pdk" remote="PixelOS" />
  <project path="system/update_engine" name="system_update_engine" groups="pdk" remote="PixelOS" />

  <!-- Begin Extras -->

  <!-- Device -->
  <project path="device/lineage/sepolicy" name="device_lineage_sepolicy" remote="PixelOS" />

  <!-- Packages -->
  <project path="packages/apps/ParanoidSense" name="packages_apps_ParanoidSense" remote="PixelOS" />
  <project path="packages/apps/TouchGestures" name="packages_apps_TouchGestures" remote="PixelOS" />
  <project path="packages/apps/Updates" name="packages_apps_Updates" remote="gl" />

  <!-- Vendor -->
  <project path="vendor/aosp" name="pixelos-vendor_aosp" remote="gl" />
  <project path="vendor/custom-preference" name="vendor_custom-preference" remote="PixelOS" />
  <project path="vendor/gms" name="vendor_gms" clone-depth="1" remote="PixelOS-Devices-Gitlab" />
  
  <project path="vendor/extra" name="vendor_extra" remote="gl" />

  <project path="lineage-sdk" name="LineageOS/android_lineage-sdk" remote="github" revision="refs/heads/lineage-21.0" />
  <project path="device/oneplus/sdm845-common" name="Grill-Laux/device_oneplus_sdm845-common" revision="refs/heads/pixelos/fourteen" remote="github" />
  <project path="device/oneplus/fajita" name="Grill-Laux/device_oneplus_fajita" revision="refs/heads/pixelos/fourteen" remote="github" />
  <project path="vendor/oneplus/sdm845-common" name="Grill-Laux/vendor_oneplus_sdm845-common" revision="refs/heads/pixelos/fourteen" remote="github" />
  <project path="vendor/oneplus/fajita" name="Grill-Laux/vendor_oneplus_fajita" revision="refs/heads/pixelos/fourteen" remote="github" />
  <project path="hardware/oneplus" name="LineageOS/android_hardware_oneplus" revision="refs/heads/lineage-21" remote="github" />
  <project path="kernel/oneplus/sdm845" name="Grill-Laux/kernel_oneplus_sdm845" revision="refs/heads/pixelos/fourteen" remote="github" />
</manifest>
